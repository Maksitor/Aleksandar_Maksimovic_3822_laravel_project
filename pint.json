{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": true,
        "single_import_per_statement": false,
        "group_import": false,
        "blank_line_after_namespace": true,
        "blank_line_before_statement": {
            "statements": ["return", "throw"]
        }
    }
}